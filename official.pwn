	if(PlayerInfo[playerid][pMuted] == 1) return errortxt(playerid, "Tkven Gadevt Chat'is Bani!");
    if(GetPVarInt(playerid, "ReportTime") > gettime()) return errortxt(playerid, "Reportis Gamoyeneba Shesadzlebelia Yovel 3Wutshi Ertxel!");
    if(reporti == 0) return infotxt(playerid, "Reporti Gamortulia Administraciis Mier!");
	if(isnull(params)) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /report [text]");
	if(!IsText(params[0])) return errortxt(playerid, "Dafiksirda Ucxo Simboloebi!");
	if(reporti == 0) return infotxt(playerid, "Report Gamortulia Shesadzloa Ikos Reorganizacia An Isvenebdes Staff !!");
	if(TotalReport == MAX_REPORTS) return errortxt(playerid, "Sachivrebis Sia Gadatvirtulia, Scadet Mogvianebit");
	new string[256];
	nfor(i, MAX_REPORTS)
	{
		if(PlayerReport[i] == -1)
		{
			strmid(TextReport[i], params[0],0,params[0],250);
			PlayerReport[i] = playerid;
			AdminReport[i] = -1;
			format(string, sizeof string, "{ffad33}[GM:RP] - {ffffff}Tqveni Kitxva/Sachivari: {FFFFFF}%s", params[0]);
   			SendClientMessage(playerid, -1, string);
      		SendClientMessage(playerid, COLOR_WHITE, "{ffad33}[GM:RP] - {ffffff}Tqveni Kitxva/Sachivari Gaigzavna, Daelodet Sanam Serveris Administracia Gipasuxebt");
			UpdateReportTD(i);
			break;
		}
	}
	TotalReport += 1;
	SetPVarInt(playerid, "ReportTime", gettime()+180);
	foreach(new i:Player)
	{
	    if(PlayerInfo[i][pAdmin] >= 1)
	    {
			GameTextForPlayer(i, "REPORT++", 1000, 1);
			RepInfoUpdate();
			TextDrawShowForPlayer(i, RepInfo[1]);
		}
	}
    return true;
}

