CMD:goto(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pAdmin] != 0)
    {
		if(sscanf(params, "u",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Daweret: /g(oto) [ID Motamashis]");
		new Float:plocx,Float:plocy,Float:plocz;
		if(!IsPlayerConnected(params[0])) return true;
		if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Motamashe Ar Shemosula");
		if(PlayerInfo[params[0]][pAdmin] >= 10 && PlayerInfo[playerid][pAdmin] < 10)
		{
 			errortxt(playerid, "Developertan Ver Mixvalt!");
		}
		else
		{
			GetPlayerPos(params[0], plocx, plocy, plocz);
			if (GetPlayerState(playerid) == 2)
			{
				new tmpcar = GetPlayerVehicleID(playerid);
				SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
				LinkVehicleToInterior(tmpcar, GetPlayerInterior(params[0]));
    			SetVehicleVirtualWorld(tmpcar, GetPlayerVirtualWorld(params[0]));
				SetPVarInt(playerid, "AntiBreik", 2);
				SetPVarInt(playerid, "AntiBreik", 2);
			}
			else
			{
				SetPlayerPos(playerid,plocx,plocy+2, plocz);
				SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(params[0]));
				SetPlayerInterior(playerid, GetPlayerInterior(params[0]));
			}
			SendClientMessage(playerid, COLOR_WHITE, "Tqven Gadateleportirdit");
		}
	}
	return true;
}
