publics: LoadGZ()
{
	new string[50];
	new File = ini_openFile("gamefiles/gangzone.ini");
	new l[2048];
	new asd[5][10];
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		format(string,sizeof(string), "Zone%d",i);
		new x = ini_getString(File,string,l);
		if(x < 0)
		{
			GZInfo[i][gCoords][0] = STANDARTGangZone[i][0];
			GZInfo[i][gCoords][1] = STANDARTGangZone[i][1];
			GZInfo[i][gCoords][2] = STANDARTGangZone[i][2];
			GZInfo[i][gCoords][3] = STANDARTGangZone[i][3];
		}
		else
		{
			split(l,asd,'|');
			GZInfo[i][gCoords][0] = floatstr(asd[0]);
			GZInfo[i][gCoords][1] = floatstr(asd[1]);
			GZInfo[i][gCoords][2] = floatstr(asd[2]);
			GZInfo[i][gCoords][3] = floatstr(asd[3]);
		}
		GZInfo[i][gFrakVlad] = strval(asd[4]);
		if(GZInfo[i][gFrakVlad] == 0) GZInfo[i][gFrakVlad] = allowedfactions[random(sizeof(allowedfactions))];
		GZInfo[i][gID] = GangZoneCreate(GZInfo[i][gCoords][0],GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]);
		ZoneOnBattle[i] = 0;
		GZSafeTime[i] = 0;
	}
	ini_closeFile(File);
}
publics: SaveGZ()
{
	new string[400];
	new File = ini_openFile("gamefiles/gangzone.ini");
	new l[10];
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		format(l,sizeof(l), "Zone%d",i);
		format(string,sizeof(string), "%f|%f|%f|%f|%d",GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3],GZInfo[i][gFrakVlad]);
		ini_setString(File,l,string);
	}
	ini_closeFile(File);
}
publics: PaintCheck()
{
	new topstr[256];
    new TopKills = -1;
	new TopPlayerID = INVALID_PLAYER_ID;
	foreach(new p:Player)
	{
		if(PaintBallKills[p] > TopKills)
		{
			TopKills = PaintBallKills[p];
			TopPlayerID = p;
		}
	}
	format(topstr, sizeof(topstr), "{0099CC}PaintBall{FFFFFF}'is Gamarjvebuli: {0099CC}%s[%d], {FFFFFF}Mas Gadaeca: {0099CC}%d$ {FFFFFF} / Kills: {0099CC}%d", Name(TopPlayerID), TopPlayerID, PaintBallMoney, TopKills);
	SendClientMessageToAll(COLOR_WHITE, topstr);
	PlayerInfo[TopPlayerID][pCash] += PaintBallMoney;
	PlayerUpdateData(TopPlayerID, "pCash", PlayerInfo[TopPlayerID][pCash]);
	foreach(new x:Player)
	{
		if(PaintBallPlayer[x] == 1)
		{
		    eventplayer[x] = false;
		    PaintBallPlayer[x] = 0;
		    PaintBallKills[x] = 0;
			PaintBallDeaths[x] = 0;
			SetPlayerPosEx(x, 2691.9851,-1697.7567,10.4953);
			SetPlayerFacingAngle(x, 205.3863);
			SetPlayerVirtualWorld(x, 0);
			SetPlayerInterior(x, 0);
			ResetWeapons(x);
			SetPlayerColor(x, TEAM_HIT_COLOR);
		}
	}
	PaintBallMoney = 0;
	PaintBallStart = 0;
	PaintBallPlayers = 0;
	return true;
}
publics: GzCheck()
{
	for(new i = 0; i < sizeof(GZInfo); i++)
	{
		if(ZoneOnBattle[i] == 1)
		{
			if(GZSafeTime[i] > 0) GZSafeTime[i] --;
            foreach(new x:Player)
 			{
				new captstats[50];
				new y = PlayerInfo[x][pMember];
				if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
				{
					if(CaptureOn[x] == 0)
					{
						switch(GZInfo[i][gNapad])
						{
							case 12:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][0], 0xCC00FFAA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][6], "hud:radar_gangP");
							}
							case 13:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][0], 0xffcd00AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][6], "hud:radar_gangY");
							}
							case 15:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][0], 0x009900AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][6], "hud:radar_gangG");
							}
							case 17:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][0], 0x00b4e1AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][6], "hud:radar_gangB");
							}
							case 18:
							{
								PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][0], 0x7777ffAA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][6], "hud:radar_gangN");
				            }
						}
						switch(GZInfo[i][gFrakVlad])
						{
							case 12:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][1], 0xCC00FFAA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][7], "hud:radar_gangP");
							}
							case 13:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][1], 0xffcd00AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][7], "hud:radar_gangY");
							}
							case 15:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][1], 0x009900AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][7], "hud:radar_gangG");
							}
							case 17:
							{
							    PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][1], 0x00b4e1AA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][7], "hud:radar_gangB");
							}
							case 18:
							{
								PlayerTextDrawColor(x, GhettoPlayerTextDraw[x][1], 0x7777ffAA);
								PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][7], "hud:radar_gangN");
				            }
				        }
				        for(new t; t != 16; t++) TextDrawShowForPlayer(x, GhettoTextDraw[t]);
				        for(new k; k != 8; k++) PlayerTextDrawShow(x, GhettoPlayerTextDraw[x][k]);
						CaptureOn[x] = 1;
					}
					if(GZSafeTime[i] > 0) format(captstats,sizeof(captstats),"TIME: %s", Convert(GZSafeTime[i]));
					PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][5], captstats);

					format(captstats,sizeof(captstats), "%s", GetGangNamec(GZInfo[i][gFrakVlad]));
					PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][1], captstats);
					format(captstats,sizeof(captstats), "%s", GetGangNamec(GZInfo[i][gNapad]));
					PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][0], captstats);

					format(captstats,sizeof(captstats), "%d", CountOnZone[GZInfo[i][gFrakVlad]]);
					PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][3], captstats);
					format(captstats,sizeof(captstats), "%d", CountOnZone[GZInfo[i][gNapad]]);
					PlayerTextDrawSetString(x, GhettoPlayerTextDraw[x][2], captstats);
				}
			}
			if(GZSafeTime[i] <= 0)
			{
			    new highscore = -1;
				new highscoreowner = INVALID_PLAYER_ID;
				foreach(new p:Player)
				{
				    new g = PlayerInfo[p][pMember];
					if(g == GZInfo[i][gNapad] || g == GZInfo[i][gFrakVlad])
					{
						if(KillCounter[p] > highscore)
						{
							highscore = KillCounter[p];
							highscoreowner = p;
						}
					}
				}
				if(CountOnZone[GZInfo[i][gFrakVlad]] > CountOnZone[GZInfo[i][gNapad]])
				{
				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
						{
						    for(new t; t != 16; t++) TextDrawHideForPlayer(x, GhettoTextDraw[t]);
						    for(new o; o != 8; o++) PlayerTextDrawHide(x, GhettoPlayerTextDraw[x][o]);
							KillCounter[x] = 0;
							DamageInformer[x] = 0;
						    PlayerInfo[x][pKills] = 0;
						    PlayerInfo[x][pDeaths] = 0;
							CaptureOn[x] = 0;
						}
					}
					SendFamilyMessage(GZInfo[i][gFrakVlad],0x00B953AA,"{FFFFFF}Gilocavt, tqven sheinarchunet tqveni teritoria");
					SendFamilyMessage(GZInfo[i][gNapad],0xC56565AA,"{FF0000}Samwuxarod tqven ver daipyarit axali teritoria");
					format(YCMDstr, 256, "Capture's sauketeso motamashe iyo: %s[%d]. man mokla %d kaci", Name(highscoreowner), highscoreowner, highscore);
					SendFamilyMessage(GZInfo[i][gFrakVlad],COLOR_WHITE,YCMDstr);
					SendFamilyMessage(GZInfo[i][gNapad],COLOR_WHITE,YCMDstr);
					CountOnZone[GZInfo[i][gFrakVlad]] = 0;
					CountOnZone[GZInfo[i][gNapad]] = 0;
				}
				else if(CountOnZone[GZInfo[i][gFrakVlad]] < CountOnZone[GZInfo[i][gNapad]])
				{
				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
						{
							for(new t; t != 16; t++) TextDrawHideForPlayer(x, GhettoTextDraw[t]);
						    for(new o; o != 8; o++) PlayerTextDrawHide(x, GhettoPlayerTextDraw[x][o]);
							KillCounter[x] = 0;
							CaptureOn[x] = 0;
						}
					}
					CountOnZone[GZInfo[i][gFrakVlad]] = 0;
					CountOnZone[GZInfo[i][gNapad]] = 0;
					SendFamilyMessage(GZInfo[i][gNapad], 0x00B953AA,"{FFFFFF}Gilocavt, tqven daipyarit axali teritoria");
					SendFamilyMessage(GZInfo[i][gFrakVlad], 0xC56565AA,"{FF0000}Samwuxarod tqven dakarget teritoriaze kontroli.");
					format(YCMDstr, 256, "Capture's sauketeso motamashe iyo: %s[%d] man mokla %d kaci", Name(highscoreowner), highscoreowner, highscore);
					SendFamilyMessage(GZInfo[i][gFrakVlad],COLOR_WHITE,YCMDstr);
					SendFamilyMessage(GZInfo[i][gNapad],COLOR_WHITE,YCMDstr);
					GZInfo[i][gFrakVlad] = GZInfo[i][gNapad];
					GZInfo[i][gNapad] = 0;
				}
				else if(CountOnZone[GZInfo[i][gFrakVlad]] == CountOnZone[GZInfo[i][gNapad]])
				{
				    GZSafeTime[i] = 60;
				    foreach(new x:Player)
		 			{
		 				new y = PlayerInfo[x][pMember];
						if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
						{
							GameTextForPlayer(x, "~y~Golden Time", 1000, 1);
						}
					}
				    return true;
				}
				captures--;
				ItsWar[GZInfo[i][gNapad]] = -1;
				ItsWar[GZInfo[i][gFrakVlad]] = -1;
				ClearTotalGz();
				ZoneOnBattle[i] = 0;
				GangZoneStopFlashForAll(GZInfo[i][gID]);
				GangZoneHideForAll(GZInfo[i][gID]);
				GangZoneShowForAll(GZInfo[i][gID],GetGangZoneColor(i));
				SaveGZ();
				if(captures == 0) KillTimer(GzCheckTimer), GzCheckOn = 0;
				break;
			}
		}
	}
	return true;
}
