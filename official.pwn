publics: UpdateBETX()
{
	if(BetX_Start < BetX_Set)
	{
	    BetX_Start += 0.008;
		new fors[20];
		format(fors,sizeof(fors),"%.2f X",BetX_Start);
	    TextDrawSetString(JetxNew_TD[10],fors);
	    foreach(new i : Player)
		{
			if(IsPlayerConnected(i) && BetX_Information[i][betX_Playing2] == true && BetX_Information[i][betX_Playing] == true)
			{
			    new fafafa[20];
			    new Float:tanxa = float(BetX_Information[i][betX_Bet])*BetX_Start;
			    format(fafafa,sizeof(fafafa),"%d$",floatround(tanxa,floatround_round));
			    PlayerTextDrawSetString(i,JetxNew_PTD[i][1],fafafa);
			}
		}
	}
	else if(BetX_Start >= BetX_Set)
	{
	    TextDrawSetString(JetxNew_TD[6],"GAME STARTING...");
	    new fors[20];
	    format(fors,sizeof(fors),"~r~%.2f X",BetX_Start);
		TextDrawSetString(JetxNew_TD[10],fors);
        RenewBetX();
		//BetX_Started = false;
		KillTimer(BetX_Timer);
		SetTimer("NullBetX",2000,false);
		foreach(new i : Player)
		{
			if(IsPlayerConnected(i) && BetX_Information[i][betX_Playing2] == true)
			{
				BetX_Information[i][betX_Playing] = false;
                BetX_Information[i][betX_Bet] = 0;
			    PlayerTextDrawSetString(i,JetxNew_PTD[i][1],"Please Place");
				PlayerTextDrawSetString(i,JetxNew_PTD[i][2],"Your Bet");
			}
		}
	}
}
publics: NullBetX(playerid)
{
	TextDrawSetString(JetxNew_TD[10],"1.00 X");
}
publics: RenewBetX()
{
	BetX_Set = floatstr(Algoritmi());
	BetX_Start = 1.00;
	BetX_Fsoni = 181.00000;
	BetX_Started = false;
	BetX_Timer2 = SetTimer("FsonebisMigeba",500,true);
}
publics: FsonebisMigeba()
{
	if(BetX_Fsoni > 2.0)
	{
	    BetX_Fsoni = BetX_Fsoni-9.00;
	}
	else
	{
  		TextDrawSetString(JetxNew_TD[6],"GAME STARTED");
	    BetX_Started = true;
		BetX_Timer = SetTimer("UpdateBETX",200,true);
	    KillTimer(BetX_Timer2);
	}
}
stock Algoritmi()
{
	new string[8];

	new rand = random(200);

	switch(rand)
	{
	    case 5..10:
	    {
			format(string, sizeof string ,"%d.%d", 2, random(60));
			return string;
	    }
	    case 25..30:
	    {
			format(string, sizeof string ,"%d.%d", 2, random(50));
			return string;
	    }
	    case 45..50:
	    {
			format(string, sizeof string ,"%d.%d", 2, random(80));
			return string;
	    }
	    case 65..70:
	    {
			format(string, sizeof string ,"%d.%d", 3, random(90));
			return string;
	    }
	    case 85..90:
	    {
			format(string, sizeof string ,"%d.%d", 3, random(90));
			return string;
	    }
	    case 105..110:
	    {
			format(string, sizeof string ,"%d.%d", 5, random(50));
			return string;
	    }
	    case 125..130:
	    {
			format(string, sizeof string ,"%d.%d", 6, random(20));
			return string;
	    }
	    case 145..150:
	    {
			format(string, sizeof string ,"%d.%d", 8, random(80));
			return string;
	    }
	    default:
	    {
			format(string, sizeof string ,"%d.%d", 1, random(50));
			return string;
	    }
	}
	format(string, sizeof string, "1.10");
	return string;
}
