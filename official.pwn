			format(b_string, sizeof(b_string), "{de5b1f}[Silver Roulette] {FFFFFF}Tkven Amogividat: {de5b1f}+%d{FFFFFF} RouletteMoney", prize);
			SendClientMessage(playerid, COLOR_WHITE, b_string);
			return true;
		}
		case 3..5:
		{
			new prize = Random(200000,6000000);
			PlayerInfo[playerid][pCash] += prize;
			PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
			format(b_string, sizeof(b_string), "{de5b1f}[Silver Roulette] {FFFFFF}Tkven Amogividat: {de5b1f}+%d{FFFFFF} Money", prize);
			SendClientMessage(playerid, COLOR_WHITE, b_string);
			return true;
		}
		case 6..8:
		{
			new prize = Random(1,2);
			PlayerInfo[playerid][pDonateRank] = prize;
			PlayerInfo[playerid][pVipTime] = getdate()+7;
			OnPlayerUpdateAc(playerid);
			format(b_string, sizeof(b_string), "{de5b1f}[Silver Roulette] {FFFFFF}Tkven Amogividat: {de5b1f}%s{FFFFFF} VIP {de5b1f}7{FFFFFF} Dgit", GetVIPName(prize));
			SendClientMessage(playerid, COLOR_WHITE, b_string);
