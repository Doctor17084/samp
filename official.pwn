publics: FuelRefresh()
{
	for(new i = 1; i <MAX_VEHICLES; i++)
	{
		GetVehicleParamsEx(i, engine, lights, alarm, doors, bonnet, boot, objective);
		if(engine == 1)
		{
			if(VehicleFuel[i] >= 1)
			{
				VehicleFuel[i] -= 1;
			}
		}
	}
	foreach(new i:Player)
    {
        if(VehicleFuel[GetPlayerVehicleID(i)] <= 0)
		{
		    if(GetVehicleModel(GetPlayerVehicleID(i)) == 481 || GetVehicleModel(GetPlayerVehicleID(i)) == 509 || GetVehicleModel(GetPlayerVehicleID(i)) == 510) return true;
