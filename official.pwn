stock IsIP(text[])
{
	new numbers;
	for(new i = 0;i < strlen(text); i++)
	{
	    if('0' <= text[i] <= '9')
	    {
	        if(!('0' <= text[i+1] <= '9'))
	        {
	            numbers ++;
	        }
	    }
	}
	if(numbers >= 4) return true;
	return false;
}
