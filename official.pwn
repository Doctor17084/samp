stock RepInfoUpdate()
{
	if(TotalReport <= 0)
	{
	    TotalReport = 0;
	}
	if(TotalReport <= 5)
	{
	    TextDrawColor(RepInfo[1], 0x00FF00FF);
	}
	if(TotalReport > 5 && TotalReport < 15)
	{
		TextDrawColor(RepInfo[1], 0xFF9900FF);
	}
	if(TotalReport > 15)
	{
		TextDrawColor(RepInfo[1], 0xFF0000FF);
	}
	new str[10];
	format(str, sizeof(str), "%d", TotalReport);
	TextDrawSetString(RepInfo[1], str);
}
stock AddDonateRequest(donater[], request[])
{
	new temp_dmd = donate_requests_made;
	format(Donate_Requester[temp_dmd], sizeof(Donate_Requester[]), "%s", donater);
	format(Donater_Want[temp_dmd], sizeof(Donater_Want[]), "%s", request);

    new
        tmp_hr, tmp_mnts, tmp_scnds, tmp_day, tmp_yr, tmp_mnth;
    gettime(tmp_hr, tmp_mnts, tmp_scnds);
    getdate(tmp_day, tmp_yr, tmp_mnth);

	format(Donater_Request_SendTime[temp_dmd], sizeof(Donater_Request_SendTime[]), "%02d/%02d/%d %02d:%02d", tmp_day, tmp_mnth, tmp_yr, tmp_hr, tmp_mnts);

	new
	    tmp_Str[124]
	;

 	format(tmp_Str, sizeof(tmp_Str), "%s - %s - %s", Donate_Requester[temp_dmd], Donater_Want[temp_dmd], Donater_Request_SendTime[temp_dmd]);
  	//SendDonateRequest(tmp_Str);

	return donate_requests_made++;
}
stock ShowDonateRequests(playerid)
{
	new tmp_dmd = donate_requests_made;

	new main_str[600], second_str[64];
	strcat(main_str, "Donater\tText\tTime\n");
	for(new i; i<tmp_dmd; i++)
	{
	    format(second_str, sizeof(second_str), "%s\t%s\t%s\n", Donate_Requester[i], Donater_Want[i], Donater_Request_SendTime[i]);
	    strcat(main_str, second_str);
	}
	ShowPlayerDialog(playerid, 1158, DIALOG_STYLE_TABLIST_HEADERS, "Donate Motxovnebi", main_str, "Clear", "");
	return 1;
}
