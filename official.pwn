		RemovePlayerAttachedObject(playerid, 1);
		SendClientMessage(playerid, COLOR_REDD,"Tqven Dagivardat Yuti");
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
	}
	if(!ispassenger) vehicleidtp[playerid] = vehicleid;
	SetVehicleParamsForPlayer(vehicleid, playerid, 0, IsLocked[vehicleid]);
	return true;
