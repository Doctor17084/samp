@PromoCodeCheck(playerid, code[]);
@PromoCodeCheck(playerid, code[])
{
	new rows;
	cache_get_row_count(rows);
	if(!rows) return errortxt(playerid, "Tkvens Mier Mititebuli PromoCode Ver Moidzebna!");
	new code_money, code_level, used_count;
    cache_get_value_index_int(0, 2, code_money);
    cache_get_value_index_int(0, 3, code_level);
    cache_get_value_index_int(0, 4, used_count);

	PlayerInfo[playerid][pPromoUse] = 1;
	PlayerUpdateData(playerid, "pPromoUse", PlayerInfo[playerid][pPromoUse]);
    PlayerInfo[playerid][pCash] += code_money;
    PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
    PlayerInfo[playerid][pLevel] += code_level;
    PlayerUpdateData(playerid, "pLevel", PlayerInfo[playerid][pLevel]);
    used_count += 1;
    new sqlsstr[180];
	format(sqlsstr, sizeof(sqlsstr), "UPDATE `"T_PROMOCODE"` SET `Used` = '%d' WHERE `Code` = '%s'", used_count, code);
	mysql_query(connects, sqlsstr);

	new promostr[256];
	format(promostr, sizeof(promostr), "{FFFFFF}Tkven Gaaktiuret PromoCode: {63BD4E}%s{FFFFFF}, Gadmogecat: {63BD4E}+%d$ {FFFFFF}& {63BD4E}+%d{FFFFFF}Level", code, code_money, code_level);
	SendClientMessage(playerid, COLOR_WHITE, promostr);
	return true;
}
