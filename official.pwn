CMD:robhouse(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(RobCarStatus[playerid] == false) return errortxt(playerid, "Tkven Ar Gyavt Naqiravebi Transporti!");
	if(RobStatus[playerid] == true) return infotxt(playerid, "Tkven Ukve Dawyebuli Gakvt Saxlis Dzarcva!");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /robhouse [House ID]");
    if(!PlayerToPoint(5, playerid, HouseInfo[params[0]][hEntrancex], HouseInfo[params[0]][hEntrancey], HouseInfo[params[0]][hEntrancez])) return errortxt(playerid, "Tkven Ar Imyofebit Saxltan Axlos!");
    if(HouseInfo[params[0]][hOwned] == 0) return errortxt(playerid, "Am Saxls Ar Yavs Mflobeli!");
    if(HouseInfo[params[0]][hLock] == 1) return errortxt(playerid, "Es Saxli Daketilia!");
    if(HouseInfo[params[0]][hRobHouseStatus] == 1) return infotxt(playerid, "Am Saxls Vigac Ukve Dzarcvavs!");
    RobStatus[playerid] = true;
    RobHouseID[playerid] = params[0];
	HouseInfo[params[0]][hRobHouseStatus] = 1;

	new Float: VehicleDistance, Float: VehiclePos[4];
 	GetVehicleModelInfo(GetVehicleModel(RobCarID[playerid]), 1, VehiclePos[0], VehicleDistance, VehiclePos[2]);
 	VehicleDistance = VehicleDistance/2 + 0.1;
 	GetVehiclePos(RobCarID[playerid], VehiclePos[0], VehiclePos[1], VehiclePos[2]);
 	GetVehicleZAngle(RobCarID[playerid], VehiclePos[3]);
 	VehiclePos[0] += (VehicleDistance * floatsin(-VehiclePos[3]+180, degrees));
 	VehiclePos[1] += (VehicleDistance * floatcos(-VehiclePos[3]+180, degrees));

 	new Float: PlayerPos[3];
	GetPlayerPos(playerid, PlayerPos[0], PlayerPos[1], PlayerPos[2]);

	RobHouseObject[playerid][0] = CreatePickupAC(19135, 23, VehiclePos[0], VehiclePos[1], VehiclePos[2]-0.5);
	RobHouseObject[playerid][1] = CreatePickupAC(19918, 23, PlayerPos[0], PlayerPos[1], PlayerPos[2]);
	return true;
}
