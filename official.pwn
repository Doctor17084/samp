			SetPlayerSkin(playerid, PlayerInfo[playerid][pChar][PlayerInfo[playerid][pViborChar]]);
			SendClientMessage(playerid, COLOR_WHITE, "{de5b1f}[Silver Roulette] {FFFFFF}Tkven Amogividat Skin ID: {de5b1f}107");
