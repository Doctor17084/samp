			{
   				SendClientMessage(playerid, COLOR_WHITE, "Am adgilshi dgas ukve boombox");
			    return true;
			}
		}
	}
	new string2[128];
	format(string2, sizeof(string2), "%s dadga boombox!", pName);
	SendNearbyMessage(playerid, 15, string2, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
