       	ApplyAnimation(playerid, "COP_AMBIENT", "Coplook_watch",4.1,0,0,0,0,0);
	}
	new string[64], mtext[20], year, month,day;
	getdate(year, month, day);
	format(string, sizeof(string), "%s'm(a) daxeda saats", Name(playerid));
	SetPlayerChatBubble(playerid, string, 0xDD90FFFF, 10.0, 5000);
	switch(month)
	{
	    case 1:mtext = "Janvari";
	    case 2: mtext = "Tebervali";
	    case 3: mtext = "Marti";
	    case 4: mtext = "April";
	    case 5:mtext = "Masisi";
	    case 6: mtext = "Ivnisi";
	    case 7: mtext = "Ivlisi";
	    case 8: mtext = "Agvisto";
	    case 9: mtext = "Seqtemberi";
	    case 10:mtext = "Oqtomberi";
	    case 11: mtext = "Novemberi";
	    case 12: mtext = "Dekemberi";
	}
	new hour,minuite,second;
	gettime(hour,minuite,second);
	if (minuite < 5)
	{
		if(PlayerInfo[playerid][pJailTime] > 0)format(string, sizeof(string), "~y~%d ~y~%s~n~~w~Saati : %d:0%d~n~ ~g~ concluded ~w~%d", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
		else format(string, sizeof(string), "~y~%d ~y~%s~n~~w~Saati : %d:0%d", day, mtext, hour, minuite);
	}
	else
	{
		if(PlayerInfo[playerid][pJailTime] > 0) format(string, sizeof(string), "~y~%d ~y~%s~n~~w~Saati : %d:%d~n~ ~g~ concluded ~w~%d", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
