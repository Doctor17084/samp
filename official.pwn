}
CMD:vipskin(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(PlayerInfo[playerid][pDonateRank] < 3) return errortxt(playerid, "Tkven Ar Gakvt Am CMD's Gamoyenebis Ufleba!");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /vipskin [Skin ID]");
    if(params[0] == 74) return true;
	if(params[0] > 299 || params[0] < 1) { SendClientMessage(playerid, COLOR_WHITE, "Araswori Skin ID"); return true; }
	SetPlayerSkin(playerid, params[0]);
	return true;
}
CMD:setskin(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pAdmin] < 9 || dostup[playerid] == 0) return true;
    if(sscanf(params, "udd", params[0], params[1], params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /setskin [Motamashis ID] [Skin ID] [Slot ID]");
	if(params[0] == INVALID_PLAYER_ID) return errortxt(playerid, "Motamashe Ver Moidzebna!");
	if(params[1] > 312 || params[1] < 1) { SendClientMessage(playerid, COLOR_GREY, "Araswori Skin ID"); return true; }
	if(params[2] < 0 || params[2] > 2) return infotxt(playerid, "Minimum: 0 / Maxsimum: 2");
	if(params[1] == 74 && !CjSkinAqvs2(Name(params[0]))) return errortxt(playerid, "Am Motamashes Ar Akvs CJ Skin'is Gamoyenebis Ufleba!");
	PlayerInfo[params[0]][pViborChar] = params[2];
	PlayerUpdateData(params[0], "pViborChar", PlayerInfo[params[0]][pViborChar]);
	PlayerInfo[params[0]][pChar][PlayerInfo[params[0]][pViborChar]] = params[1];
	PlayerUpdateData(params[0], "pChar", PlayerInfo[params[0]][pChar][0]);
	PlayerUpdateData(params[0], "pChar1", PlayerInfo[params[0]][pChar][1]);
	PlayerUpdateData(params[0], "pChar2", PlayerInfo[params[0]][pChar][2]);
	SetPlayerSkin(params[0], PlayerInfo[params[0]][pChar][PlayerInfo[params[0]][pViborChar]]);
	format(b_string, sizeof(b_string), "Administrator: {de5b1f}%s{FFFFFF} | Gadmogcat Skin ID: {de5b1f}%d[%d]", Name(playerid), params[1], params[2]);
	SendClientMessage(params[0], COLOR_WHITE, b_string);
	return true;
}
