		if(BattlePosition[i] == 1 && MaffiaWarTime[i] == 0)
		{
			ZoneTimerm[i] -= 10;
			if(ZoneTimerm[i] <= 0)
			{
				for(new h = 1; h <= TotalBizz; h++)
				{
					if(BizzInfo[h][bCapture] == 1)
					{
						BizzInfo[h][bMafia] = BizzInfo[h][bMafia];
						SendFamilyMessage(BizzInfo[h][bMafia], COLOR_GREEN, "{009933}[Informacia] {FFFFFF}Gilocavt! Tkven Shedzelit Biznesze Kontrolis Shenarchuneba!");
						BizzInfo[h][bCapture] = 0;
					}
				}
				SendFamilyMessage(MZInfo[i][mNapad], COLOR_WHITE, "{AE433D}[Informacia] {FFFFFF}Samwuxarod Tkven Ver Shedzelit Axali Biznesis Dapyroba!");
				StartWar = 0;
				BattlePosition[i] = 0;
				GangZoneStopFlashForAll(WarCoordinats);
			}
			foreach(new x:Player)
 			{
				if(PlayerToKvadrat(x, 2311, -692.5, 2390, -628.5) && IsACrime(x))
				{
					new factionms;
					if(PlayerInfo[x][pLeader] != 0) factionms = PlayerInfo[x][pLeader];
					else factionms = PlayerInfo[x][pMember];
					WarZone[i][factionms] ++;
				}
			}
			new asd;
			for(new x = 0; x < 20; x++) if(WarZone[i][x] > 0 && IsInAllowedM(x)) asd++;
			if(asd == 1)
			{
				for(new x =0;x<20;x++)
				{
					if(WarZone[i][x] > 0)
					{
						new zx,zl;
						zx = x;
						zl = i;
						if(MZInfo[i][mFrakVlad] == x)
						{
							for(new h = 1; h <= TotalBizz; h++)
							{
								if(BizzInfo[h][bCapture] == 1)
								{
									BizzInfo[h][bMafia] = BizzInfo[h][bMafia];
									BizUpdateData(h, "bMafia", BizzInfo[h][bMafia]);
									SendFamilyMessage(BizzInfo[h][bMafia], COLOR_WHITE, "{009933}[Informacia] {FFFFFF}Gilocavt! Tkven Shedzelit Biznesze Kontrolis Shenarchuneba!");
									BizzInfo[h][bCapture] = 0;
								}
							}
							StartWar = 0;
							SendFamilyMessage(MZInfo[i][mNapad], COLOR_WHITE, "{AE433D}[Informacia] {FFFFFF}Samwuxarod Tkven Ver Shedzelit Axali Biznesis Dapyroba!");
						}
						else
						{
							StartWar = 0;
							SendFamilyMessage(MZInfo[i][mNapad], COLOR_WHITE, "{009933}[Informacia] {FFFFFF}Gilocavt! Tkven Shedzelit Axali Biznesis Dapyroba!");
							for(new h = 1; h <= TotalBizz; h++)
							{
								if(BizzInfo[h][bCapture] == 1)
								{
									SendFamilyMessage(BizzInfo[h][bMafia], COLOR_WHITE, "{AE433D}[Informacia] {FFFFFF}Samwuxarod Tkven Ver Shedzelit Biznesis Shenarchuneba!");
									BizzInfo[h][bMafia] = MZInfo[i][mNapad];
									BizUpdateData(h, "bMafia", BizzInfo[h][bMafia]);
									BizzInfo[h][bCapture] = 0;
								}
							}
						}
						BattlePosition[zl] =0;
						MZInfo[zl][mFrakVlad] = 0;
						MZInfo[zl][mFrakVlad] = zx;
						GangZoneStopFlashForAll(WarCoordinats);
						GangZoneHideForAll(WarCoordinats);
						KillTimer(WarTimer);
