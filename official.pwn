CMD:setname(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pAdmin] < 10 || dostup[playerid] == 0) return true;
    {
		if(sscanf(params, "us[64]", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /setname [Motamashis ID] [Saxeli]");
		if(params[0] == INVALID_PLAYER_ID) return errortxt(playerid, "Motamashe Ver Moidzebna!");
		if(PlayerInfo[params[0]][pCarStatus] != 0) return errortxt(playerid, "Am Motamashis Transporti Dgas Gasayidad!");
