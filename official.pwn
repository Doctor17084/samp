		        format(carlist, sizeof(carlist), "[1] %s\n[2] %s\n[3] %s\n[4] %s\n[5] %s", VehicleNames[PlayerInfo[playerid][pCar][0]-400], VehicleNames[PlayerInfo[playerid][pCar][1]-400], VehicleNames[PlayerInfo[playerid][pCar][2]-400], VehicleNames[PlayerInfo[playerid][pCar][3]-400], VehicleNames[PlayerInfo[playerid][pCar][4]-400]);
