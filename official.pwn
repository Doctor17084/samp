CMD:awarn(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pAdmin] < 9 || dostup[playerid] == 0) return true;
	if(sscanf(params, "us[256]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "{E3801D}[Daxmareba]{FFFFFF} format: /awarn [Admin ID] [Reason]");
	if(!IsPlayerConnected(params[0])) return errortxt(playerid, "Administrators Jer Ar Gauvlia Avtorizacia!");
	if(PlayerInfo[params[0]][pAdmin] >= 10) return SendClientMessage(playerid, COLOR_REDD, "Serveris Owners Ver Dauwert AWarns !");
	if(PlayerInfo[params[0]][pAdmin] < 1) return errortxt(playerid, "Es Motamashe Ar Aris Administratori!");
    PlayerInfo[params[0]][pAdminWarning] += 1;
	if(PlayerInfo[params[0]][pAdminWarning] >= 3)
	{
		format(YCMDstr, sizeof(YCMDstr), "[A] %s'm(a) Moxsna Administratoris Postidan: %s [%d/3] [Reason: %s]", Name(playerid), Name(params[0]), PlayerInfo[params[0]][pAdminWarning], params[1]);
		SendAdminMessage(COLOR_YELLOW,YCMDstr);
		PlayerInfo[params[0]][pAdmin] = 0;
		PlayerInfo[params[0]][pAdminWarning] = 0;
		return true;
	}
	format(YCMDstr, sizeof(YCMDstr), "[A] %s'm(a) Misca Gafrtxileba Administrators: %s [%d/3] [Reason: %s]", Name(playerid), Name(params[0]), PlayerInfo[params[0]][pAdminWarning], params[1]);
	SendAdminMessage(COLOR_YELLOW,YCMDstr);
	return true;
}
