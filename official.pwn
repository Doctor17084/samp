						mysql_query(connects, sql_str);
					}
				}
				if(PenaltyInfo[p][CarIDInfo] != -1)
				{
					DestroyVehicle(PenaltyInfo[p][CarIDInfo]);
					PenaltyInfo[p][CarIDInfo] = -1;
				}
