	}
    PlayerInfo[playerid][pCash] -= 60000000;
    PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
    PlayerInfo[playerid][pFamilyMember] = TotalFamily;
    PlayerUpdateData(playerid, "pFamilyMember", PlayerInfo[playerid][pFamilyMember]);
    PlayerInfo[playerid][pFamilyRank] = 6;
    PlayerUpdateData(playerid, "pFamilyRank", PlayerInfo[playerid][pFamilyRank]);

    format(otherstr, sizeof(otherstr), "INSERT INTO "T_FAMILY" (FamilyID, FamilyName, FamilyLeader, FamilyCoLeader ) VALUES (%d, '%s', '%s', 'None')", TotalFamily, inputtext, Name(playerid));
    mysql_tquery(connects, otherstr, "", "");

    FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyID] = TotalFamily;
	FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyHouseID] = 999;
	FamilyUpdateData(PlayerInfo[playerid][pFamilyMember], "FamilyHouseID", FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyHouseID]);
	FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilySkinID] = 230;
	FamilyUpdateData(PlayerInfo[playerid][pFamilyMember], "FamilySkinID", FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilySkinID]);
    format(FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyName], 20, inputtext);
	format(FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyLeader], MAX_PLAYER_NAME, Name(playerid));
	strmid(FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyCoLeader], "None", 0, strlen("None"), 20);
	FamilyUpdateDataString(PlayerInfo[playerid][pFamilyMember], "FamilyCoLeader", FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyCoLeader]);

	if(PlayerInfo[playerid][pFamilyText] == Text3D:INVALID_3DTEXT_ID)
	{
		if(PlayerInfo[playerid][pFamilyMember] != 0)
		{
		    new textstr[150];
		    format(textstr,sizeof(textstr), "{%s}%s{FFFFFF} - %s", FamilyColors[FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyColor]], FamilyTypes[FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyType]], FamilyInfo[PlayerInfo[playerid][pFamilyMember]][FamilyName]);
			PlayerInfo[playerid][pFamilyText] = CreateDynamic3DTextLabel(textstr, COLOR_WHITE, 0.0, 0.0, 0.4, 6.0,  playerid, INVALID_VEHICLE_ID, 1 );
		}
	}
	return true;
}
@LoadFamily();
@LoadFamily()
{
    new time = GetTickCount();
    new rows;
	cache_get_row_count(rows);
	if(rows)
	{
	    for(new i = 1; i <= rows; i++)
		{
		    cache_get_value_index_int(i-1, 0, FamilyInfo[i][FamilyID]);
		    cache_get_value_name(i-1, "FamilyName", FamilyInfo[i][FamilyName], 20);
			cache_get_value_name(i-1, "FamilyLeader", FamilyInfo[i][FamilyLeader], MAX_PLAYER_NAME);
			cache_get_value_name(i-1, "FamilyCoLeader", FamilyInfo[i][FamilyCoLeader], MAX_PLAYER_NAME);
			cache_get_value_index_int(i-1, 4, FamilyInfo[i][FamilyType]);
			cache_get_value_index_int(i-1, 5, FamilyInfo[i][FamilyColor]);
			cache_get_value_index_int(i-1, 6, FamilyInfo[i][FamilyHouseID]);
			cache_get_value_index_int(i-1, 7, FamilyInfo[i][FamilySkinID]);
			cache_get_value_index_int(i-1, 8, FamilyInfo[i][FamilyMaterials]);
			if(FamilyInfo[i][FamilySkinID] == 186)
			{
			    FamilyInfo[i][FamilySkinID] = 230;
			    FamilyUpdateData(i, "FamilySkinID", FamilyInfo[i][FamilySkinID]);
			}
			TotalFamily++;
        }
    }
	printf("[FAMILY LOADED]: <%i>. TIME: <%i ms>.", TotalFamily, GetTickCount() - time);
	return true;
}
