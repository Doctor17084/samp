@PromoCodeCreate(playerid, code[]);
@PromoCodeCreate(playerid, code[])
{
	new rows;
	cache_get_row_count(rows);
	if(rows >= 10) return infotxt(playerid, "Serverze Ukve Maxsimaluri Raodenobis PromoCode Aris Shekmnili");
    new load_code[32], free_slot = -1;
    for(new i; i < rows; i++)
    {
        cache_get_value_index(i, 0, load_code, 32);
        if(!strcmp(load_code, code, false))
        {
			free_slot = i;
        	break;
  		}
    }

    if(free_slot != -1)
	{
		errortxt(playerid, "Tkvens Mier Mititebuli PromoCode Ukve Arsebobs!");
		DeletePVar(playerid, "PromoName");
		DeletePVar(playerid, "PromoLevel");
		DeletePVar(playerid, "PromoCash");
		return false;
	}
    new PromoLevel = GetPVarInt(playerid, "PromoLevel");
    new PromoCash = GetPVarInt(playerid, "PromoCash");

	new promocodestr[300];
    format(promocodestr, sizeof(promocodestr), "INSERT INTO "T_PROMOCODE" (Code, Money, Level) VALUES ('%s', %d, %d)", code, PromoCash, PromoLevel);
    mysql_tquery(connects, promocodestr, "", "");

    SendClientMessage(playerid, COLOR_WHITE, "{de5b1f}[Create PromoCode] {FFFFFF}Tkvens Mier Archeuli PromoCode Warmatebit Sheikmna!");

	DeletePVar(playerid, "PromoName");
	DeletePVar(playerid, "PromoLevel");
	DeletePVar(playerid, "PromoCash");
	return true;
}
