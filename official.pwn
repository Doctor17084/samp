publics: SalonTimer()
{
	for(new c = 1; c <= TotalCar; c++)
	{
	    if(CarInfo[c][CarStatus] == 1 && CarInfo[c][CarTime] == 1)
	    {
	        CarInfo[c][CarTime] -= 1;
	        CarUpdateData(c, "CarTime", CarInfo[c][CarTime]);
	        CarUpdateText(c);
	    }
	    else if(CarInfo[c][CarStatus] == 1 && CarInfo[c][CarTime] <= 0)
	    {
	    	new target;
			target = ReturnUser(CarInfo[c][CarOwner]);
			if(target >= 0 && target != INVALID_PLAYER_ID)
			{
				PlayerInfo[target][pCarStatus] = 0;
				PlayerUpdateData(target, "pCarStatus", PlayerInfo[target][pCarStatus]);
			}
			else
			{
       			format(sql_str, sizeof(sql_str), "UPDATE accounts SET pCarStatus = '0' WHERE Name = '%s'", CarInfo[c][CarOwner]);
				mysql_query(connects, sql_str);
			}
			CarInfo[c][CarID] = 462;
			CarUpdateData(c, "CarID", CarInfo[c][CarID]);
			CarInfo[c][CarPrice] = 0;
			CarUpdateData(c, "CarPrice", CarInfo[c][CarPrice]);
			CarInfo[c][CarStatus] = 0;
			CarUpdateData(c, "CarStatus", CarInfo[c][CarStatus]);
			CarInfo[c][CarSlot] = 0;
			CarUpdateData(c, "CarSlot", CarInfo[c][CarSlot]);
			CarInfo[c][CarTime] = 0;
			CarUpdateData(c, "CarTime", CarInfo[c][CarTime]);
			strmid(CarInfo[c][CarOwner], "None", 0, strlen("None"), 20);
			CarUpdateDataString(c, "CarOwner", CarInfo[c][CarOwner]);
			CarUpdateText(c);
			if(CarInfo[c][CarIDInfo] != -1)
			{
				DestroyVehicle(CarInfo[c][CarIDInfo]);
				CarInfo[c][CarIDInfo] = -1;
			}
	    }
	}
 	for(new p = 1; p <= TotalPenalty; p++)
	{
	    if(PenaltyInfo[p][CarStatus] == 1 && PenaltyInfo[p][CarTime] >= 1)
	    {
	        PenaltyInfo[p][CarTime] -= 1;
	        PenaltyUpdateData(p, "CarTime", PenaltyInfo[p][CarTime]);
	        PenaltyUpdateText(p);
	        if(PenaltyInfo[p][CarStatus] == 1 && PenaltyInfo[p][CarTime] == 0)
	        {
		    	new target;
				target = ReturnUser(PenaltyInfo[p][CarOwner]);
				if(target >= 0 && target != INVALID_PLAYER_ID)
				{
				    format(YCMDstr, sizeof(YCMDstr), "{de5b1f}[Police Message] {FFFFFF}Tkveni Transporti: {de5b1f}%s{FFFFFF} Gaiyida Saxelmwifos Mier", VehicleNames[PenaltyInfo[p][CarID]-400]);
					SendClientMessage(target, COLOR_WHITE, YCMDstr);
					if(PenaltyInfo[p][CarSlot] == 1)
					{
					    TuningOFF(target, 1);
					    PlayerInfo[target][pCar][0] = 462;
					    PlayerUpdateData(target, "pCar", PlayerInfo[target][pCar][0]);
					    PlayerInfo[target][pCarPenalty][0] = 0;
						PlayerUpdateData(target, "pCarPenalty1", PlayerInfo[target][pCarPenalty][0]);
					}
					else if(PenaltyInfo[p][CarSlot] == 2)
					{
					    TuningOFF(target, 2);
					    PlayerInfo[target][pCar][1] = 462;
					    PlayerUpdateData(target, "pCar1", PlayerInfo[target][pCar][1]);
					    PlayerInfo[target][pCarPenalty][1] = 0;
						PlayerUpdateData(target, "pCarPenalty2", PlayerInfo[target][pCarPenalty][1]);
					}
					else if(PenaltyInfo[p][CarSlot] == 3)
					{
					    TuningOFF(target, 3);
					    PlayerInfo[target][pCar][2] = 462;
					    PlayerUpdateData(target, "pCar2", PlayerInfo[target][pCar][2]);
					    PlayerInfo[target][pCarPenalty][2] = 0;
						PlayerUpdateData(target, "pCarPenalty3", PlayerInfo[target][pCarPenalty][2]);
					}
					else if(PenaltyInfo[p][CarSlot] == 4)
					{
					    TuningOFF(target, 4);
					    PlayerInfo[target][pCar][3] = 462;
					    PlayerUpdateData(target, "pCar3", PlayerInfo[target][pCar][3]);
					    PlayerInfo[target][pCarPenalty][3] = 0;
						PlayerUpdateData(target, "pCarPenalty4", PlayerInfo[target][pCarPenalty][3]);
					}
					else if(PenaltyInfo[p][CarSlot] == 5)
					{
					    TuningOFF(target, 5);
					    PlayerInfo[target][pCar][4] = 462;
					    PlayerUpdateData(target, "pCar4", PlayerInfo[target][pCar][4]);
					    PlayerInfo[target][pCarPenalty][4] = 0;
						PlayerUpdateData(target, "pCarPenalty5", PlayerInfo[target][pCarPenalty][4]);
					}
				}
				else
				{
					if(PenaltyInfo[p][CarSlot] == 1)
					{
		       			format(sql_str, sizeof(sql_str), "UPDATE accounts SET pCarPenalty1 = '0', pCar = '462' WHERE Name = '%s'", PenaltyInfo[p][CarOwner]);
						mysql_query(connects, sql_str);
