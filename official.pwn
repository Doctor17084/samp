}
publics: GPSUPDATEInformation(playerid)
{
	new Float:PlayerPos[3], minstr[32];
	GetPlayerPos(playerid, PlayerPos[0], PlayerPos[1], PlayerPos[2]);
	new distance = GetDistanceBetweenPoints(PlayerPos[0], PlayerPos[1], PlayerPos[2], GPSPos[playerid][0], GPSPos[playerid][1], GPSPos[playerid][2]);
	format(minstr, sizeof(minstr), "%d_~w~m", distance);
	PlayerTextDrawSetString(playerid, GPSPTXT[1][playerid], minstr);
	new Float: complete_precent = 100.0 - (distance / GPSDistance[playerid] * 100.0);
	new Float: line_size = 54.6 / 100.0 * complete_precent;
	if(line_size < 0.0) line_size = 0.0;
	PlayerTextDrawTextSize(playerid, GPSPTXT[2][playerid], line_size, 11.199996);
	PlayerTextDrawHide(playerid, GPSPTXT[2][playerid]);
	PlayerTextDrawShow(playerid, GPSPTXT[2][playerid]);
}
