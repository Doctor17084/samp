    GPSPos[playerid][0] = PosX, GPSPos[playerid][1] = PosY, GPSPos[playerid][2] = PosZ, GPSDistance[playerid] = GetDistanceBetweenPoints(PlayerPos[0], PlayerPos[1], PlayerPos[2], PosX, PosY, PosZ);
