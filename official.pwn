						GameTextForPlayer(playerid, l_string, 1000, 1);
						RadarMoney += RadarInfo[i][RadarCash];
					}
					else if(PlayerInfo[playerid][pCash] < RadarInfo[i][RadarCash] && PlayerInfo[playerid][pBank] >= RadarInfo[i][RadarCash] && PlayerInfo[playerid][pAdmin] == 0 && !IsACop(playerid))
