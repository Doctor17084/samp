publics: RouletteGoldPrize(playerid)
{
    SetPVarInt(playerid, "RouletteStart", 0);
    PlayerTextDrawSetSelectable(playerid, NewRoulette[playerid][25], 1);
    PlayerTextDrawSetSelectable(playerid, NewRoulette[playerid][15], 1);
    PlayerTextDrawSetSelectable(playerid, NewRoulette[playerid][20], 1);
    PlayerTextDrawSetSelectable(playerid, NewRoulette[playerid][0], 1);
    PlayerTextDrawHide(playerid, NewRoulette[playerid][25]);
    PlayerTextDrawHide(playerid, NewRoulette[playerid][15]);
    PlayerTextDrawHide(playerid, NewRoulette[playerid][20]);
    PlayerTextDrawShow(playerid, NewRoulette[playerid][25]);
    PlayerTextDrawShow(playerid, NewRoulette[playerid][15]);
    PlayerTextDrawShow(playerid, NewRoulette[playerid][20]);
    PlayerTextDrawHide(playerid, NewRoulette[playerid][0]);
    PlayerTextDrawShow(playerid, NewRoulette[playerid][0]);
	KillTimer(RouletteTimer[playerid]);
	RouletteTimer[playerid] = -1;
	PlayerPlaySound(playerid, 31205, 0.0, 0.0, 0.0);
	switch(random(14))
	{
		case 0..2:
		{
		    new prize = Random(5,10);
			PlayerInfo[playerid][pRouletteMoney] += prize;
			PlayerUpdateData(playerid, "pRouletteMoney", PlayerInfo[playerid][pRouletteMoney]);
			format(l_string, sizeof(l_string), "%d", PlayerInfo[playerid][pRouletteMoney]);
			PlayerTextDrawSetString(playerid, NewRoulette[playerid][2], l_string);
			format(b_string, sizeof(b_string), "{de5b1f}[Gold Roulette] {FFFFFF}Tkven Amogividat: {de5b1f}+%d{FFFFFF} RouletteMoney", prize);
			SendClientMessage(playerid, COLOR_WHITE, b_string);
			return true;
		}
		case 3..5:
		{
			new prize = Random(300000,7000000);
			PlayerInfo[playerid][pCash] += prize;
			PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
			format(b_string, sizeof(b_string), "{de5b1f}[Gold Roulette] {FFFFFF}Tkven Amogividat: {de5b1f}+%d{FFFFFF} Money", prize);
			SendClientMessage(playerid, COLOR_WHITE, b_string);
			return true;
		}
		case 6..8:
		{
