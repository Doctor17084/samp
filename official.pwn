		        ShowPlayerDialog(playerid, 1357, DIALOG_STYLE_LIST, "{de5b1f}Donate Gift{FFFFFF} | Choose Slot", carlist, "Archeva", "Daxurva");
		    }
		    else if(car == 2)
		    {
		        SendClientMessage(playerid, COLOR_WHITE, "Tkven Amogividat Sachukarshi: {63BD4E}Turismo{FFFFFF}'is Mankana");
		        SetPVarInt(playerid, "GiftCar", 451);
		        new carlist[180];
		        format(carlist, sizeof(carlist), "[1] %s\n[2] %s\n[3] %s\n[4] %s\n[5] %s", VehicleNames[PlayerInfo[playerid][pCar][0]-400], VehicleNames[PlayerInfo[playerid][pCar][1]-400], VehicleNames[PlayerInfo[playerid][pCar][2]-400], VehicleNames[PlayerInfo[playerid][pCar][3]-400], VehicleNames[PlayerInfo[playerid][pCar][4]-400]);
		        ShowPlayerDialog(playerid, 1357, DIALOG_STYLE_LIST, "{de5b1f}Donate Gift{FFFFFF} | Choose Slot", carlist, "Archeva", "Daxurva");
		    }
		    else if(car == 3)
		    {
		        SendClientMessage(playerid, COLOR_WHITE, "Tkven Amogividat Sachukarshi: {63BD4E}PCJ-600{FFFFFF}'is MotoBike");
		        SetPVarInt(playerid, "GiftCar", 461);
