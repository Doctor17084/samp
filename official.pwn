	if(!IsPlayerInAnyVehicle(playerid))
	{
		if(!IsPlayerInRangeOfPoint(playerid, 20.0, oldPos[playerid][0], oldPos[playerid][1], oldPos[playerid][2]))
		{
			new str[256]; format(str, 256, "{FF5F5F}[AntiCheat] {ffffff} Motamashe %s[%d] Mivida Checkpoint'tan Chetis Gamokenebit",Name(playerid),playerid);
			SendAdminMessage(-1, str);
		}
	}

	if(FormaFrac[playerid] == 1)
	{
		foreach(new i:Player)
		{
		    if(gotome[i] == playerid)
		    {
		        ClearAnimations(playerid);
		        SendClientMessage(playerid, COLOR_GREY, "Tqven Miatrevt Damnashaves Da Ver Chajdebit Manqanashi, Jer Daweret: /gotome [ID Motamashis]");
		        return true;
			}
		}
	}
	if(IsPlayerInAnyVehicle(playerid)) return TrollDetect(playerid);
    if(IsLocked[vehicleid] == 1)
	{
		ClearAnimations(playerid);
	}
	if(IsLocked[vehicleid] == 1)
	{
	    LockCar(vehicleid);
		GameTextForPlayer(playerid,"~w~Manqana ~r~Daketilia", 3000, 3);
		ClearAnim(playerid);
	}
	if(vehicle_RadioState[vehicleid] == true)
	{
	    new
	        temp_vRadioURL[2048];
	    GetPlayerAudioInVehicle(vehicleid, temp_vRadioURL);
	    PlayAudioStreamForPlayer(playerid, temp_vRadioURL);
	}
	if(!ispassenger)
	{
		if(IsVehicleHasDriver(vehicleid))
		{
			SendClientMessage(playerid,-1,"Motamashis transportidan gadmogdeba akrdzalulia!");
			new Float:X,Float:Y,Float:Z;
			GetPlayerPos(playerid,X,Y,Z);
			SetPlayerPosEx(playerid,X+0.1,Y+0.1,Z+0.1);
			SetPVarInt(playerid, "AntiBreik", 1);
			ClearAnimations(playerid);
			return true;
		}
		if(caridhouse[playerid] != vehicleid)
		{
			new vehowner = GetVehicleOwner(vehicleid);
			if(vehowner != INVALID_PLAYER_ID)
			{
			    if(keyfloods[playerid] == false)
			    {
			        keyfloods[playerid] = true;
					new exQuery[350],name[28],nametarget[28];
