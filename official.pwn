cmd:forsale(playerid, params[])
{
    new ForSaleDialogString[4000],houseID,houseKlass,houseValue;
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	format(YCMDstr, sizeof(YCMDstr), "SELECT * FROM `house` WHERE hOwned = '0' LIMIT 30");
	mysql_query(connects,YCMDstr);
	new rows;
	cache_get_row_count(rows);
	if(!rows) return SendClientMessage(playerid,COLOR_GREY,"Gasakidi Saxlebi Ver Moidzebna");
	strcat(ForSaleDialogString, "House ID\tHouse Class\tHouse Value\n");
	for new i = 0;i < rows; i++ do
	{
		cache_get_value_name_int(i,"hID",houseID);
		cache_get_value_name_int(i,"hKlass",houseKlass);
		cache_get_value_name_int(i,"hValue",houseValue);
		format(YCMDstr, sizeof(YCMDstr), "{FF9900}ID: {FFFFFF}%d\t{FF9900}Class Done: {FFFFFF}%d\t{FF9900}Value: {FFFFFF}%d\n", houseID, houseKlass, houseValue);
		strcat(ForSaleDialogString, YCMDstr);
	}
	SendClientMessage(playerid, COLOR_WHITE, "{FF9900}[Info] {FFFFFF}Sasurveli Saxlis Mosadzebnad Daweret /House [Saxlis ID]");
	ShowPlayerDialog(playerid, 7979, DIALOG_STYLE_TABLIST_HEADERS, "{FF9900}Houses For Sale", ForSaleDialogString, "{FF9900}OK", "");
	return true;
}
